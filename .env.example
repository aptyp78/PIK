## Adobe PDF Services — product token (uploads-only)
ADOBE_CLIENT_ID=
ADOBE_CLIENT_SECRET=
# Optional region override (e.g., ew1, ue1). Leave empty for default
ADOBE_REGION=

## Unstructured Platform API base (workflows)
UNSTRUCTURED_API_URL=https://platform.unstructuredapp.io/api/v1
UNSTRUCTURED_API_KEY=
UNSTRUCTURED_WORKFLOW_ID=
UNS_SOURCE_CONNECTOR_ID=
UNS_DEST_QDRANT_ID=
DRIVE_FILE_ID=
DRIVE_FILENAME_REGEX=^PIK 5-0 - Platform Business Model - ENG\.pdf$
BM_SOURCE=qdrant

# Default engine for /upload (server) — 'unstructured' | 'adobe'
INGEST_ENGINE_DEFAULT=unstructured

# Client-side default (UI hint); optional
NEXT_PUBLIC_INGEST_ENGINE_DEFAULT=unstructured

## Qdrant (read-only viewer)
QDRANT_URL=
QDRANT_COLLECTION=PIK-Unstructured
QDRANT_API_KEY_RO=

## Optional
# OPENAI_API_KEY=

# Protect ingestion in production (optional, not required for dev)
# INGEST_TOKEN=

## GCS Results (destinations)
# Bucket for results written by workflows and parsers
GCS_RESULTS_BUCKET=
# Object prefix for Unstructured→GCS destination that mirrors Qdrant
GCS_QDRANT_DEST_PREFIX=
# Object prefix for Adobe parsing outputs
GCS_ADOBE_DEST_PREFIX=
